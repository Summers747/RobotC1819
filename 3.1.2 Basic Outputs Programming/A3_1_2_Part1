task main()
{                                     //Program begins, insert code within curly braces
	turnLEDOff(green);
	wait(1);
	turnLEDOn(green);
	wait(1);
	turnLEDOff(green);
	wait(1);
	turnLEDOn(green);
	wait(1);
	turnLEDOff(green);
}
