task main()
{                               //Program begins, insert code within curly braces
	setServo(servoMotor, -127);
	wait(2);
	setServo(servoMotor, -63);
	wait(3);
	setServo(servoMotor, 0);
	wait(2);
	setServo(servoMotor, 63);
	wait(3);
	setServo(servoMotor, 127);
	wait(2);
}
